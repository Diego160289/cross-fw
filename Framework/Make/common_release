CXXFLAGS =	-O2 -Wall -fmessage-length=0

CPP =		ModuleCounter.cpp

OBJS =		ModuleCounter.o

INCLUDE =		./Include

CPPS =		./Sources/Common/$(CPP)

OBJECTS =		./Objects/Release/Common/$(OBJS)

TARGET =		libCommon.a

common_release: $(OBJECTS)
	ar cr ./Libs/Release/$(TARGET) $(OBJECTS)

$(OBJECTS): $(CPPS)
	g++ $(CXXFLAGS) -c $(CPPS) -I $(INCLUDE) -o $(OBJECTS)
